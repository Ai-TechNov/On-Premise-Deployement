#copy this file to /etc/systemd/system/
[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=root
Group=root
WorkingDirectory=/app2
Environment="PATH=/app2/app2env/bin"
ExecStart=/app2/app2env/bin/gunicorn --workers 3 --bind=0.0.0.0:5000  wsgi

[Install]
WantedBy=multi-user.target
